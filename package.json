{
  "name": "rainfall-tracking",
  "version": "0.0.1",
  "description": "Simple rainfal tracker with postgres",
  "main": "index.js",
  "repository": "git@github.com:Rennzie/rainfall-tracking.git",
  "author": "rennzie",
  "license": "MIT",
  "scripts": {
    "start:server": "NODE_ENV=development nodemon --exec babel-node server.js",
    "start:client": "cd client && yarn start:client",
    "seed-test": "knex seed:run --env test",
    "test": "yarn seed-test && NODE_ENV=test nyc mocha --require @babel/register __tests__/**/*.test.js --require __tests__/test_helper.js --recursive --exit ",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn clean && babel src --preset minify --out-dir dist"
  },
  "dependencies": {
    "apollo-server": "^2.3.1",
    "apollo-server-lambda": "^2.3.3",
    "atob": "^2.1.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "global": "^4.3.2",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "objection": "^1.4.0",
    "objection-guid": "^3.0.2",
    "pg": "^7.7.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
