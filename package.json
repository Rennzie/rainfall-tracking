{
  "name": "rainfall-tracking",
  "version": "0.0.1",
  "description": "Simple rainfal tracker with postgres",
  "main": "index.js",
  "repository": "git@github.com:Rennzie/rainfall-tracking.git",
  "author": "rennzie",
  "license": "MIT",
  "scripts": {
    "start:server": "nodemon index.js",
    "start:client": "cd client && yarn start:client",
    "clean": "rm -rf dist && mkdir dist",
    "seed": "babel-node src/db/seeds",
    "build": "yarn clean && babel src --preset minify --out-dir dist",
    "heroku-postbuild": "yarn build && cd client && yarn && yarn build",
    "start": "node dist/index.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "debug": "node ./testRef.js",
    "test": "nyc mocha --require @babel/register test/**/*_spec.js --require test/spec_helper --recursive --exit"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "objection": "^1.4.0",
    "pg": "^7.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.8.3",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  }
}
